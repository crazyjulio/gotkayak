<p id="notice"><%= notice %></p>

<h1>Fish</h1>

<table>
  <thead>
    <tr>
      <th>Species</th>
      <th>Length</th>
      <th>Weight</th>
      <th>Location</th>
      <th>Water type</th>
      <th>Catch date</th>
      <th>Comment</th>
      <th>Disputed by</th>
      <th>User comment</th>
      <th>User</th>
      <th>Images</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fish.each do |fish| %>
      <tr>
        <td><%= fish.species %></td>
        <td><%= fish.length %></td>
        <td><%= fish.weight %></td>
        <td><%= fish.location %></td>
        <td><%= fish.water_type %></td>
        <td><%= fish.catch_date %></td>
        <td><%= fish.comment %></td>
        <td><%= fish.disputed_by %></td>
        <td><%= fish.user_comment %></td>
        <td><%= fish.user_id %></td>
        <td><%= fish.images %></td>
        <td><%= link_to 'Show', fish %></td>
        <td><%= link_to 'Edit', edit_fish_path(fish) %></td>
        <td><%= link_to 'Destroy', fish, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fish', new_fish_path %>
